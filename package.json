{
  "name": "@flux0-ai/react",
  "version": "0.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Flux0 React toolkit.",
  "type": "commonjs",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "exports": {
    "./test-utils": {
      "import": {
        "types": "./dist/test-utils/index.d.mts",
        "default": "./dist/test-utils/index.mjs"
      },
      "require": {
        "types": "./dist/test-utils/index.d.ts",
        "default": "./dist/test-utils/index.js"
      }
    },
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "dev:package": "tsup --watch",
    "build": "tsup",
    "postbuild": "attw --pack --profile node16 && publint",
    "lint": "biome check .",
    "dev": "run-p dev:*",
    "dev:storybook": "pnpm --filter storybook dev",
    "storybook:build": "pnpm --filter storybook build",
    "test": "vitest"
  },
  "keywords": [
    "openai",
    "gpt",
    "langchain",
    "langgraph",
    "pyndanticai",
    "llm",
    "react",
    "component",
    "hooks",
    "sse",
    "useStream"
  ],
  "author": "Asaf Shakarzy",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flux0-ai/flux0-react.git"
  },
  "lint-staged": {
    "*.{js,json,css,md,ts,tsx}": [
      "biome check --apply --no-errors-on-unmatched --files-ignore-unknown=true"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "beta",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "packageManager": "pnpm@10.3.0",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.4",
    "@biomejs/biome": "^1.9.4",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitest/browser": "^3.0.7",
    "@vitest/coverage-istanbul": "^3.0.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.4.3",
    "msw": "^2.7.3",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.50.1",
    "publint": "^0.3.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.7"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "@microsoft/fetch-event-source": "^2.0.1",
    "mini-rfc6902": "^0.2.3"
  }
}
